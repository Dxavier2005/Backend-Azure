{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [],
    "watchAssets": false
  },
  "exclude": [
    "**/*.tsx",
    "src/pages/**",
    "src/components/**",
    "src/context/**",
    "src/api/**",
    "src/styles/**",
    "src/main.tsx",
    "src/App.tsx"
  ]
}
